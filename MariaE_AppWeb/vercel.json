{
    "version": 2,
    "builds": [
      {
        "src": "MariaE_API/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "Front-End/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/MariaE_API/app.js" },
      { "src": "/(.*)", "dest": "/Front-End/$1" }
    ]
  }
  