{
  "version": 2,
  "builds": [
    {
      "src": "MariaE_API/routes/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front-End/html/index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "Front-End/html" }
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "MariaE_API/routes/server.js" },
    { "src": "/", "dest": "Front-End/html/index.html" },
    { "src": "/(.*)", "dest": "Front-End/html/index.html" }
  ]
}
